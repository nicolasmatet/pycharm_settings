<application>
  <component name="GitToolBoxAppSettings2">
    <option name="commitMessageValidationConfig">
      <CommitMessageValidationConfig>
        <option name="enabled" value="true" />
        <option name="regexConfig">
          <RegexConfig>
            <option name="expression" value="(?:chore|ci|docs|feat|fix|merge|perf|refactor|revert|style|test)(?:\(.+\))?:\s.+(\n(\n.+(.|\n)*.+\n?)?)(\n(?:see|fix):\s.+\n?)*?\Z" />
            <option name="multiline" value="true" />
            <option name="testInput" value="fix(beta): test&#10;&#10;dkjfhksdjhf&#10;kjddjkf&#10;" />
          </RegexConfig>
        </option>
      </CommitMessageValidationConfig>
    </option>
    <option name="commitMessageValidationEnabled" value="true" />
    <option name="version" value="6" />
  </component>
</application>