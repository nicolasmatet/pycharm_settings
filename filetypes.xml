<application>
  <component name="FileTypeManager" version="18">
    <ignoreFiles list="*$py.class;*.pyc;*.pyo;*.rbc;*.yarb;*~;.DS_Store;.git;.hg;.svn;CVS;__pycache__;_svn;vssver.scc;vssver2.scc" />
    <extensionMap>
      <mapping pattern="elect pg_terminate_backend(pid) " type="AUTO_DETECTED" />
      <mapping pattern="v2" type="AUTO_DETECTED" />
      <mapping ext="npy" type="PLAIN_TEXT" />
      <removed_mapping ext="txt" approved="true" type="PLAIN_TEXT" />
    </extensionMap>
  </component>
</application>